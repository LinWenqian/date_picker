/**index.wxss**/
.index_page{
  position: relative;
  width: 100%;
  height: 100vh;
}

.selectedDate,
.selectedRangeDate{
  display: flex;
  margin: 10rpx;
}

.selectedDate>input,
.selectedRangeDate>input{
  width: 50%;
  border:1px solid #e2e2e2;
}

.date_picker_box{
  position: fixed;
  bottom: 0;
  z-index: 99;
  background-color: #ffffff;
  border-top: 1px solid #C9C9C9;
}